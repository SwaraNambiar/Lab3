//find out if the number is prime or not
#include <stdio.h>
int main ()
{
    int i,n;
    printf("Enter any number");
    scanf("%d",n);
    for (int i=1; i<=(n/2); i++)
    {
        if (n%i!=0)
        {printf("The number %d is not prime",n);}
        else {printf("The number %d is prime",n);}
    }
}